        <DebugType>full</DebugType>
        <DebugSymbols>true</DebugSymbols>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="MDD4All.SVG.DataModels" Version="3.0.0.3" />
    </ItemGroup>
    
</Project>
